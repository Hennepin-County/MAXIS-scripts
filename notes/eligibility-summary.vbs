	Next

	If UBound(GRH_UNIQUE_APPROVALS, 2) <> 0 Then
		Dialog1 = ""
		BeginDialog Dialog1, 0, 0, 241, 215, UBound(GRH_UNIQUE_APPROVALS, 2)+1 & " HS/GRH Approval Packages to be Reviewed"
		  GroupBox 5, 10, 145, 40, "REVIEW ALL APPROVAL PACKAGES"
